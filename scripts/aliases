alias awsunset='unset $(env | grep AWS_ | egrep -o "^[^=]+")'
alias gensecret='openssl rand -base64 32'
alias mkpass='pwgen -sn1 16'
alias myip='dig @ns1.google.com o-o.myaddr.l.google.com TXT +short -4 | tr -d \" | tr -d "\n"'
alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'
alias sshkey='cat ~/.ssh/id_rsa.pub'
alias tgclear='find . -type d -name ".terragrunt-cache" -prune -exec rm -rf {} \;'
alias tg='terragrunt'